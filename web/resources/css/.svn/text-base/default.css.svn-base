/*********************************Button*****************************************************/
.booked-Button{
    background: #EF2F12 url("") repeat-x;
    margin: 0;
    border: 1px solid #999999;
    font-weight: bold;
    padding: 7px 10px 7px 10px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-box-shadow: 0 1px 2px #444;
    -webkit-box-shadow: 0 1px 2px #444;
    box-shadow: 0 1px 2px #444;
    text-shadow: 1px 0 0 #cccccc;
    text-align: center;
    color: #ffffff;
    font-size: 10px;

    height: 35px;
}
.unbooked-Button{

    height: 35px;
    background: #2EAA14 url("") repeat-x;
    margin: 0;
    border: 1px solid #999999;
    font-weight: bold;
    padding: 7px 10px 7px 10px;
