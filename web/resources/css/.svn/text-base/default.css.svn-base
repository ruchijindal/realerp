

/***********************Font Resizing******************/
.ui-widget {
    font-size: .75em;

}

/************************HEADER************************/

#header {
    clear: both;
    overflow: none;
    background: url("../images/bg-header.png");
    height: 60px;
    background-repeat: repeat-x;

}
#smplogo {
    background-image:url("../images/smplogo.png");
    height: 50px;
    float: left;
    width: 100px;
    text-indent:-9999px;
    display: block;
    margin: 5px 0px 5px 5px;
    background-repeat: no-repeat;
}

#logo {
    background-image:url("../images/logo-realestate.png");
    height: 50px;
    float: left;
    width: 320px;
    text-indent:-9999px;
    display: block;
    margin: 5px 0px 5px 5px;
}

#company-info {
    font-size: 12px;
    float: right;
    height: 30px;
    margin: 5px 5px 5px 0px;
}

#company-info ul {
    list-style: none;
}

/*********** FOOTER*************/
#footer {
    clear: both;
    overflow: none;
    background: url("../images/bg-header.png");
    height: 25px;
    background-repeat: repeat-x;
    padding:0.2em 0.2em 0.5em 0.0em;

}

#logout {
    font-size: 12px;
    float: right;
    height: 30px;

}
#logout ul {
    list-style: none;

}

/***************************** for wizard tab buttorn **********************/

.ui-wizard-step-title{
    font-size: 95%;
}



